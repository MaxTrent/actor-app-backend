    "http-status": "^1.7.4",
    "joi": "^17.11.0",
    "jose": "^5.2.4",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
