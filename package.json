{
  "name": "actor-app-backend",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "node .esbuild/esbuild.config.cjs",
    "build:watch": "node .esbuild/esbuild-watchmode.cjs",
    "start": "npm run clean && npm run build && node --enable-source-maps dist/index.js",
    "start:watch": "nodemon -q --enable-source-maps dist/index.js",
    "predev": "npm run build",
    "dev": "cross-env NODE_ENV=development concurrently -n 'server,build' \"npm run start:watch\" \"npm run build:watch\"",
    "format": "echo 'Running format script' && prettier --write **/*.ts",
    "prod": "set NODE_ENV=production && node dist/index.js",
    "test": "npm run test",
    "prepare": "husky install"
  },
  "dependencies": {
