    "@sentry/profiling-node": "^1.3.4",
    "@types/node": "^20.12.11",
    "@types/socket.io": "^3.0.2",
    "@vinejs/vine": "^2.0.0",
    "ai": "^2.2.31",
    "app.ts": "^0.1.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.22.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "esbuild": "^0.20.2",
    "esbuild-node-externals": "^1.13.1",
    "express": "^4.18.2",
